{
  "name": "jest-chance",
  "version": "0.1.11",
  "description": "Random seed generator for chancejs with jest",
  "exports": {
    "types": "./dist/types/index.d.ts",
    "import": "./dist/esm/index.js",
    "require": "./dist/cjs/index.js"
  },
  "files": [
    "dist/"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "type": "module",
  "private": false,
  "repository": "https://github.com/meza/jest-chance",
  "license": "MIT",
  "scripts": {
    "clean:githooks": "lefthook uninstall -k",
    "ci": "pnpm test && pnpm build",
    "lint": "eslint --ext ts src",
    "lint:fix": "eslint --fix --ext ts src",
    "test": "pnpm lint && vitest run --coverage && pnpm build",
    "build": "tsc -p tsconfig.json && tsc -p tsconfig-cjs.json",
    "semantic-release": "semantic-release",
    "release": "semantic-release"
  },
  "dependencies": {
    "chance": "^1.1.9"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^14.1.0",
    "@meza/tsconfig-base": "^1.1.0",
    "@semantic-release/changelog": "^6.0.1",
    "@types/chance": "^1.1.3",
    "@types/node": "^16.18.0",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "@vitest/coverage-c8": "^0.28.0",
    "c8": "^7.12.0",
    "commitlint": "^14.1.0",
    "eslint": "^8.26.0",
    "eslint-config-tailored-tunes": "^5.0.2",
    "eslint-plugin-jest": "^25.7.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-security": "^1.5.0",
    "install-deps-postmerge": "^2.0.1",
    "lefthook": "^1.1.3",
    "pnpm": "^8.0.0",
    "semantic-release": "^19.0.5",
    "semantic-release-export-data": "^1.0.1",
    "typescript": "^4.8.4",
    "vitest": "^0.28.0"
  },
  "engines": {
    "node": ">=19.0.0"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
